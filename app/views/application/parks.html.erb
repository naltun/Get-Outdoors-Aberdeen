<table class="table table-hover">
  <tr>
    <th>Name</th>
    <th>Opening Time</th>
    <th>Closing Time<th>
    <th>Address</th>
    <th>Bus Routes</th>
    <th>Description</th>
  </tr>
  <% Park.all.each do |p| %>  
    <tr>
      <td><%= p.name %></td>
      <td><%= p.openTime %></td>
      <td><%= p.closeTime %></td>
      <td><%= p.address %></td>
      <td><%= p.busRoutes %></td>
      <td><%= p.description %></td>
    </tr>
  <% end %>
</table>
